<UserControl x:Class="WPF.InfoPorAnoView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:WPF"
             
             xmlns:sys="clr-namespace:System;assembly=mscorlib"
             
			 mc:Ignorable="d ignore"
             xmlns:ignore="DavidMora/ignore/UsarParaNoCompilarElElemento"
             xmlns:p="clr-namespace:WPF.Properties"
             xmlns:conv="clr-namespace:WPF.Converters"
             xmlns:be="clr-namespace:WPF.Behaviors"
             xmlns:uc="clr-namespace:WPF.Components"
             xmlns:i="http://schemas.microsoft.com/xaml/behaviors"
             xmlns:mvvm="clr-namespace:MvvmFramework;assembly=MvvmFramework"
             ignore:DataContext="{d:DesignInstance Type=local:InfoPorAnoViewModel}"
             d:DataContext="{d:DesignInstance Type=local:DesignTimeViewModel, IsDesignTimeCreatable=True}"
			 
             xmlns:gif="http://wpfanimatedgif.codeplex.com"
             gif:ImageBehavior.AnimateInDesignMode="True"
             
             d:Background="{StaticResource AppBackground}"
             d:DesignHeight="2000" d:DesignWidth="1000">

    <UserControl.InputBindings>
        <KeyBinding Gesture="Control+1" Command="{Binding MultiploCommand}">
            <KeyBinding.CommandParameter>
                <sys:Int32>1</sys:Int32>
            </KeyBinding.CommandParameter>
        </KeyBinding>
        <KeyBinding Gesture="Control+2" Command="{Binding MultiploCommand}">
            <KeyBinding.CommandParameter>
                <sys:Int32>1000</sys:Int32>
            </KeyBinding.CommandParameter>
        </KeyBinding>
        <KeyBinding Gesture="Control+3" Command="{Binding MultiploCommand}">
            <KeyBinding.CommandParameter>
                <sys:Int32>1000000</sys:Int32>
            </KeyBinding.CommandParameter>
        </KeyBinding>
    </UserControl.InputBindings>

    <UserControl.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Themes/HelpTextDictionary.xaml"/>
            </ResourceDictionary.MergedDictionaries>
            
            <!--#region CONSTANTES-->
            <sys:Int32 x:Key="ColAyuda">0</sys:Int32>
            <sys:Int32 x:Key="ColConcepto">1</sys:Int32>
            <sys:Int32 x:Key="ColMiles">2</sys:Int32>
            <sys:Int32 x:Key="ColMillones">3</sys:Int32>
            <sys:Int32 x:Key="ColAno9">4</sys:Int32>
            <sys:Int32 x:Key="ColAno8">5</sys:Int32>
            <sys:Int32 x:Key="ColAno7">6</sys:Int32>
            <sys:Int32 x:Key="ColAno6">7</sys:Int32>
            <sys:Int32 x:Key="ColAno5">8</sys:Int32>
            <sys:Int32 x:Key="ColAno4">9</sys:Int32>
            <sys:Int32 x:Key="ColAno3">10</sys:Int32>
            <sys:Int32 x:Key="ColAno2">11</sys:Int32>
            <sys:Int32 x:Key="ColAno1">12</sys:Int32>
            <sys:Int32 x:Key="ColAno0">13</sys:Int32>
            <sys:Int32 x:Key="ColGrafica">14</sys:Int32>

            <sys:Int32 x:Key="RowIngresos">0</sys:Int32>
            <sys:Int32 x:Key="RowIngUnidad">1</sys:Int32>
            <sys:Int32 x:Key="RowCrecIngP">2</sys:Int32>
            <sys:Int32 x:Key="RowCostoVentas">3</sys:Int32>
            <sys:Int32 x:Key="RowUBruta">4</sys:Int32>
            <sys:Int32 x:Key="RowMargenBruto">5</sys:Int32>
            <sys:Int32 x:Key="RowGastosOp">6</sys:Int32>
            <sys:Int32 x:Key="RowUOperativa">7</sys:Int32>
            <sys:Int32 x:Key="RowMargenOp">8</sys:Int32>
            <sys:Int32 x:Key="RowGastosFin">9</sys:Int32>
            <sys:Int32 x:Key="RowUAImpuestos">10</sys:Int32>
            <sys:Int32 x:Key="RowImpuestos">11</sys:Int32>
            <sys:Int32 x:Key="RowUNeta">12</sys:Int32>
            <sys:Int32 x:Key="RowUAccionistas">13</sys:Int32>
            <sys:Int32 x:Key="RowCrecUtP">14</sys:Int32>
            <sys:Int32 x:Key="RowMargenNeto">15</sys:Int32>

            <sys:Int32 x:Key="RowIngresosProp">0</sys:Int32>
            <sys:Int32 x:Key="RowNOI">1</sys:Int32>
            <sys:Int32 x:Key="RowFFO">4</sys:Int32>
            <sys:Int32 x:Key="RowAFFO">7</sys:Int32>
            <sys:Int32 x:Key="RowNOIAccion">2</sys:Int32>
            <sys:Int32 x:Key="RowFFOAccion">5</sys:Int32>
            <sys:Int32 x:Key="RowAFFOAccion">8</sys:Int32>
            <sys:Int32 x:Key="RowMargenNOI">3</sys:Int32>
            <sys:Int32 x:Key="RowMargenFFO">6</sys:Int32>
            <sys:Int32 x:Key="RowMargenAFFO">9</sys:Int32>

            <sys:Int32 x:Key="RowEfectivo">0</sys:Int32>
            <sys:Int32 x:Key="RowDeuda">1</sys:Int32>
            <sys:Int32 x:Key="RowActivosC">2</sys:Int32>
            <sys:Int32 x:Key="RowPasivosC">3</sys:Int32>
            <sys:Int32 x:Key="RowActivosT">4</sys:Int32>
            <sys:Int32 x:Key="RowPasivosT">5</sys:Int32>
            <sys:Int32 x:Key="RowCapital">6</sys:Int32>
            <sys:Int32 x:Key="RowCrecCapP">7</sys:Int32>
            <sys:Int32 x:Key="RowACPC">8</sys:Int32>
            <sys:Int32 x:Key="RowATPT">9</sys:Int32>
            <sys:Int32 x:Key="RowPTAT">10</sys:Int32>
            <sys:Int32 x:Key="RowPAcido">11</sys:Int32>
            <sys:Int32 x:Key="RowEfDeuda">12</sys:Int32>
            <sys:Int32 x:Key="RowEfCapital">13</sys:Int32>
            <sys:Int32 x:Key="RowDeudaAT">14</sys:Int32>
            <sys:Int32 x:Key="RowDeudaCapital">15</sys:Int32>

            <sys:Int32 x:Key="RowFOperaciones">1</sys:Int32>
            <sys:Int32 x:Key="RowFInversiones">2</sys:Int32>
            <sys:Int32 x:Key="RowFFinanciamiento">3</sys:Int32>
            <sys:Int32 x:Key="RowEfInicio">0</sys:Int32>
            <sys:Int32 x:Key="RowEfFinal">4</sys:Int32>

            <sys:Int32 x:Key="RowAccCirc">0</sys:Int32>
            <sys:Int32 x:Key="RowPrecio">1</sys:Int32>
            <sys:Int32 x:Key="RowCapMerc">2</sys:Int32>
            <sys:Int32 x:Key="RowUPA">3</sys:Int32>
            <sys:Int32 x:Key="RowPU">4</sys:Int32>
            <sys:Int32 x:Key="RowValLibro">5</sys:Int32>
            <sys:Int32 x:Key="RowPVL">6</sys:Int32>
            <sys:Int32 x:Key="RowDividendos">7</sys:Int32>
            <sys:Int32 x:Key="RowRendDividendos">8</sys:Int32>
            <!--#endregion-->
            
            <Style x:Key="AnoLabel" TargetType="{x:Type Label}"
               BasedOn="{StaticResource EncabezadoLabel_App}">
                <Setter Property="ContentStringFormat" Value="{}{0:yyyy}"/>
                <Style.Triggers>
                    <Trigger Property="Content" Value="{x:Null}">
                        <Setter Property="Visibility" Value="Collapsed"/>
                    </Trigger>
                </Style.Triggers>
            </Style>
            <Style x:Key="FechaLabel" TargetType="{x:Type Label}"
               BasedOn="{StaticResource SubEncabezadoLabel_App}">
                <Setter Property="ContentStringFormat" Value="{}{0:MMM}"/>
            </Style>

            <Style x:Key="MilesLabel" TargetType="{x:Type Label}"
               BasedOn="{StaticResource MilesLabel_App}">
                <Setter Property="Content" Value="[Miles]"/>
                <Setter Property="Grid.Column" Value="{StaticResource ColMiles}"/>
            </Style>
            <Style x:Key="MillonesLabel" TargetType="{x:Type Label}"
               BasedOn="{StaticResource MillonesLabel_App}">
                <Setter Property="Content" Value="[Millones]"/>
                <Setter Property="Grid.Column" Value="{StaticResource ColMillones}"/>
            </Style>

            <Style x:Key="ConceptoLabel" TargetType="{x:Type Label}"
               BasedOn="{StaticResource ConceptoLabel_App}">
                <Setter Property="Grid.Column" Value="{StaticResource ColConcepto}"/>
            </Style>

            <Style x:Key="UnidadLabel" TargetType="{x:Type Label}"
               BasedOn="{StaticResource UnidadLabel_App}">
                <Style.Triggers>
                    <Trigger Property="Content" Value="{x:Null}">
                        <Setter Property="Visibility" Value="Collapsed"/>
                    </Trigger>
                </Style.Triggers>
            </Style>
            <Style x:Key="PorcentajeLabel" TargetType="{x:Type Label}"
               BasedOn="{StaticResource PorcentajeLabel_App}">
                <Style.Triggers>
                    <Trigger Property="Content" Value="{x:Null}">
                        <Setter Property="Visibility" Value="Collapsed"/>
                    </Trigger>
                </Style.Triggers>
            </Style>
            <Style x:Key="MonedaLabel" TargetType="{x:Type Label}"
               BasedOn="{StaticResource MonedaLabel_App}">
                <Style.Triggers>
                    <Trigger Property="Content" Value="{x:Null}">
                        <Setter Property="Visibility" Value="Collapsed"/>
                    </Trigger>
                </Style.Triggers>
            </Style>
            <Style x:Key="Moneda4Label" TargetType="{x:Type Label}"
               BasedOn="{StaticResource Moneda4Label_App}">
                <Style.Triggers>
                    <Trigger Property="Content" Value="{x:Null}">
                        <Setter Property="Visibility" Value="Collapsed"/>
                    </Trigger>
                </Style.Triggers>
            </Style>

            <Style TargetType="GroupBox">
                <Setter Property="BorderThickness" Value="1"/>
                <Setter Property="FontWeight" Value="Bold"/>
                <Setter Property="BorderBrush" Value="{StaticResource AppBackground}"/>
            </Style>

            <Style TargetType="CheckBox">
                <Setter Property="ContentStringFormat" Value="{}{0:yyyy}"/>
                <Setter Property="FontWeight" Value="Normal"/>
                <Setter Property="Visibility" Value="Visible"/>
                <Style.Triggers>
                    <Trigger Property="Content" Value="{x:Null}">
                        <Setter Property="Visibility" Value="Collapsed"/>
                    </Trigger>
                </Style.Triggers>
            </Style>

            <Style x:Key="AllButton" TargetType="Button">
                <Setter Property="Margin" Value="0,5,5,0"/>
                <Setter Property="FontWeight" Value="Normal"/>
                <!--<Setter Property="Height" Value="24"/>
                <Setter Property="Content">
                    <Setter.Value>
                        <Image Style="{x:Null}" Height="24" 
                               Source="/Images/TodosChecked.png" VerticalAlignment="Top" Margin="0,-5,0,0"/>
                    </Setter.Value>
                </Setter>-->
            </Style>
            <Style x:Key="NoneButton" TargetType="Button">
                <Setter Property="Margin" Value="0,5,0,0"/>
                <Setter Property="FontWeight" Value="Normal"/>
                <!--<Setter Property="Height" Value="24"/>
                <Setter Property="Content">
                    <Setter.Value>
                        <Image Style="{x:Null}" Height="24" VerticalAlignment="Top" Margin="0,-5,0,0"
                               Source="/Images/NingunoChecked.png"/>
                    </Setter.Value>
                </Setter>-->
            </Style>
            
            <Style TargetType="Image">
                <Setter Property="HorizontalAlignment" Value="Left"/>
                <Setter Property="VerticalAlignment" Value="Center"/>
                <Setter Property="Margin" Value="8,0,0,0"/>
                <Setter Property="Height" Value="26"/>
                
                <!--<Setter Property="gif:ImageBehavior.AnimatedSource" Value="Images/Warning.gif"/>-->
                <!--<Setter Property="gif:ImageBehavior.RepeatBehavior" Value="3x"/>-->
                <Setter Property="Visibility" Value="Collapsed"/>
                <Style.Triggers>
                    <Trigger Property="Tag" Value="1">
                        <Setter Property="Visibility" Value="Visible"/>
                        <Setter Property="gif:ImageBehavior.AnimatedSource" Value="Images/Good.gif"/>
                    </Trigger>
                    <Trigger Property="Tag" Value="-1">
                        <Setter Property="Visibility" Value="Visible"/>
                        <Setter Property="gif:ImageBehavior.AnimatedSource" Value="Images/Warning.gif"/>
                    </Trigger>
                </Style.Triggers>
            </Style>

            <Style x:Key="EncabezadoGrid" TargetType="{x:Type Grid}">
                <Setter Property="Margin" Value="12,0,30,0"/>
            </Style>
            
            <!--#region COLUMNDEFINITION-->
            <Style x:Key="AyudaColumnDefinition" TargetType="{x:Type ColumnDefinition}">
                <Setter Property="Width" Value="Auto"/>
                <Setter Property="SharedSizeGroup" Value="Ayuda"/>
            </Style>
            <Style x:Key="ConceptoColumnDefinition" TargetType="{x:Type ColumnDefinition}">
                <Setter Property="Width" Value="Auto"/>
                <Setter Property="SharedSizeGroup" Value="Concepto"/>
            </Style>
            <Style x:Key="Ano9ColumnDefinition" TargetType="{x:Type ColumnDefinition}">
                <Setter Property="Width" Value="Auto"/>
                <Setter Property="SharedSizeGroup" Value="Ano9"/>
                <Style.Triggers>
                    <DataTrigger Binding="{Binding IsChecked, ElementName=EnableColAno9}" Value="False">
                        <Setter Property="Width" Value="0"/>
                    </DataTrigger>
                </Style.Triggers>
            </Style>
            <Style x:Key="Ano8ColumnDefinition" TargetType="{x:Type ColumnDefinition}">
                <Setter Property="Width" Value="Auto"/>
                <Setter Property="SharedSizeGroup" Value="Ano8"/>
                <Style.Triggers>
                    <DataTrigger Binding="{Binding IsChecked, ElementName=EnableColAno8}" Value="False">
                        <Setter Property="Width" Value="0"/>
                    </DataTrigger>
                </Style.Triggers>
            </Style>
            <Style x:Key="Ano7ColumnDefinition" TargetType="{x:Type ColumnDefinition}">
                <Setter Property="Width" Value="Auto"/>
                <Setter Property="SharedSizeGroup" Value="Ano7"/>
                <Style.Triggers>
                    <DataTrigger Binding="{Binding IsChecked, ElementName=EnableColAno7}" Value="False">
                        <Setter Property="Width" Value="0"/>
                    </DataTrigger>
                </Style.Triggers>
            </Style>
            <Style x:Key="Ano6ColumnDefinition" TargetType="{x:Type ColumnDefinition}">
                <Setter Property="Width" Value="Auto"/>
                <Setter Property="SharedSizeGroup" Value="Ano6"/>
                <Style.Triggers>
                    <DataTrigger Binding="{Binding IsChecked, ElementName=EnableColAno6}" Value="False">
                        <Setter Property="Width" Value="0"/>
                    </DataTrigger>
                </Style.Triggers>
            </Style>
            <Style x:Key="Ano5ColumnDefinition" TargetType="{x:Type ColumnDefinition}">
                <Setter Property="Width" Value="Auto"/>
                <Setter Property="SharedSizeGroup" Value="Ano5"/>
                <Style.Triggers>
                    <DataTrigger Binding="{Binding IsChecked, ElementName=EnableColAno5}" Value="False">
                        <Setter Property="Width" Value="0"/>
                    </DataTrigger>
                </Style.Triggers>
            </Style>
            <Style x:Key="Ano4ColumnDefinition" TargetType="{x:Type ColumnDefinition}">
                <Setter Property="Width" Value="Auto"/>
                <Setter Property="SharedSizeGroup" Value="Ano4"/>
                <Style.Triggers>
                    <DataTrigger Binding="{Binding IsChecked, ElementName=EnableColAno4}" Value="False">
                        <Setter Property="Width" Value="0"/>
                    </DataTrigger>
                </Style.Triggers>
            </Style>
            <Style x:Key="Ano3ColumnDefinition" TargetType="{x:Type ColumnDefinition}">
                <Setter Property="Width" Value="Auto"/>
                <Setter Property="SharedSizeGroup" Value="Ano3"/>
                <Style.Triggers>
                    <DataTrigger Binding="{Binding IsChecked, ElementName=EnableColAno3}" Value="False">
                        <Setter Property="Width" Value="0"/>
                    </DataTrigger>
                </Style.Triggers>
            </Style>
            <Style x:Key="Ano2ColumnDefinition" TargetType="{x:Type ColumnDefinition}">
                <Setter Property="Width" Value="Auto"/>
                <Setter Property="SharedSizeGroup" Value="Ano2"/>
                <Style.Triggers>
                    <DataTrigger Binding="{Binding IsChecked, ElementName=EnableColAno2}" Value="False">
                        <Setter Property="Width" Value="0"/>
                    </DataTrigger>
                </Style.Triggers>
            </Style>
            <Style x:Key="Ano1ColumnDefinition" TargetType="{x:Type ColumnDefinition}">
                <Setter Property="Width" Value="Auto"/>
                <Setter Property="SharedSizeGroup" Value="Ano1"/>
                <Style.Triggers>
                    <DataTrigger Binding="{Binding IsChecked, ElementName=EnableColAno1}" Value="False">
                        <Setter Property="Width" Value="0"/>
                    </DataTrigger>
                </Style.Triggers>
            </Style>
            <Style x:Key="Ano0ColumnDefinition" TargetType="{x:Type ColumnDefinition}">
                <Setter Property="Width" Value="Auto"/>
                <Setter Property="SharedSizeGroup" Value="Ano0"/>
            </Style>
            <Style x:Key="GraficaColumnDefinition" TargetType="{x:Type ColumnDefinition}">
                <Setter Property="Width" Value="Auto"/>
                <Setter Property="SharedSizeGroup" Value="Grafica"/>
            </Style>
            <!--#endregion-->
            
            <Style x:Key="BorderRowOdd" TargetType="{x:Type Border}">
                <Setter Property="Grid.Column" Value="1"/>
                <Setter Property="Grid.ColumnSpan" Value="15"/>
                <Setter Property="Margin" Value="2,2,3,2"/>
                <Setter Property="CornerRadius" Value="0,10,10,0"/>
                <Setter Property="Background" Value="#1EFFFFFF"/>
            </Style>
            <Style x:Key="BorderRowPair" TargetType="{x:Type Border}">
                <Setter Property="Grid.Column" Value="1"/>
                <Setter Property="Grid.ColumnSpan" Value="15"/>
                <Setter Property="Margin" Value="3,3,4,3"/>
                <Setter Property="CornerRadius" Value="0,10,10,0"/>
            </Style>
            <Style x:Key="BorderRowUtilidad" TargetType="{x:Type Border}">
                <Setter Property="Grid.Column" Value="1"/>
                <Setter Property="Grid.ColumnSpan" Value="15"/>
                <Setter Property="Margin" Value="3,3,4,3"/>
                <Setter Property="CornerRadius" Value="0,10,10,0"/>
                <Setter Property="BorderThickness" Value="1"/>
                <Setter Property="BorderBrush" Value="#FF909090"/>
                <Setter Property="Background" Value="#0CFFF304"/>
            </Style>
            <Style x:Key="BorderColumn" TargetType="{x:Type Border}">
                <Setter Property="Grid.Row" Value="0"/>
                <Setter Property="Grid.RowSpan" Value="20"/>
                <Setter Property="Margin" Value="0"/>
                <Setter Property="CornerRadius" Value="20,20,20,20"/>
                <Setter Property="BorderThickness" Value="4,0,4,0"/>
                <Setter Property="BorderBrush" Value="#FF909090"/>
                <Setter Property="Background" Value="Transparent"/>
                <Style.Triggers>
                    <Trigger Property="Tag" Value="{x:Null}">
                        <Setter Property="Visibility" Value="Collapsed"/>
                    </Trigger>
                </Style.Triggers>
            </Style>

            <Style TargetType="uc:PoligonChart">
                <Setter Property="Cursor" Value="Hand"/>
                <Setter Property="Width" Value="115"/>
                <Setter Property="Height" Value="25.96"/>
                <Setter Property="MinWidth" Value="115"/>
                <Setter Property="MinHeight" Value="25.96"/>
                <Style.Triggers>
                    <EventTrigger RoutedEvent="MouseDown">
                        <EventTrigger.Actions>
                            <BeginStoryboard>
                                <Storyboard>
                                    <DoubleAnimation Storyboard.TargetProperty="Width"
                                                                 To="230"
                                                                 Duration="0:0:0.5"/>
                                    <DoubleAnimation Storyboard.TargetProperty="Height"
                                                                 To="77.88"
                                                                 Duration="0:0:0.5"/>
                                </Storyboard>
                            </BeginStoryboard>
                        </EventTrigger.Actions>
                    </EventTrigger>
                    <EventTrigger RoutedEvent="MouseLeave">
                        <EventTrigger.Actions>
                            <BeginStoryboard>
                                <Storyboard>
                                    <DoubleAnimation Storyboard.TargetProperty="Width"
                                                                 To="115"
                                                                 Duration="0:0:0.5"/>
                                    <DoubleAnimation Storyboard.TargetProperty="Height"
                                                                 To="25.96"
                                                                 Duration="0:0:0.5"/>
                                </Storyboard>
                            </BeginStoryboard>
                        </EventTrigger.Actions>
                    </EventTrigger>
                </Style.Triggers>
            </Style>
            
            <ControlTemplate x:Key="info" TargetType="ToggleButton">
                <Grid Width="16"  Margin="2,0,2,0">
                    <Image Source="Images/Info.png"
                           Cursor="Hand">
                        <Image.Style>
                            <Style TargetType="Image">
                                <Setter Property="Opacity" Value="0.5"/>
                                <Style.Triggers>
                                    <Trigger Property="IsMouseOver" Value="True">
                                        <Setter Property="Opacity" Value="1"/>
                                    </Trigger>
                                </Style.Triggers>
                            </Style>
                        </Image.Style>
                    </Image>
                    <Popup IsOpen="{Binding IsChecked, RelativeSource={RelativeSource TemplatedParent}}"
                                    StaysOpen="False"
                                    Placement="Bottom"
                                    HorizontalOffset="{Binding ActualWidth, RelativeSource={RelativeSource TemplatedParent}}"
                                    PopupAnimation="Slide"
                                    AllowsTransparency="True">
                        <Border Background="#FFF0F0D5"
                                CornerRadius="0,10,10,10"
                                Padding="10,6"
                                BorderThickness="1"
                                BorderBrush="Black" >
                            <ContentPresenter Content="{Binding DataContext, RelativeSource={RelativeSource TemplatedParent}}"/>
                        </Border>
                    </Popup>
                </Grid>
            </ControlTemplate>
            <ControlTemplate x:Key="settings" TargetType="ToggleButton">
                <Grid Margin="5">
                    <Image Source="Images/Visible.png"
                                       Cursor="Hand">
                        <Image.Style>
                            <Style TargetType="Image">
                                <Setter Property="Opacity" Value="0.75"/>
                                <Style.Triggers>
                                    <Trigger Property="IsMouseOver" Value="True">
                                        <Setter Property="Opacity" Value="1"/>
                                    </Trigger>
                                </Style.Triggers>
                            </Style>
                        </Image.Style>
                    </Image>
                    <Popup IsOpen="{Binding IsChecked, RelativeSource={RelativeSource TemplatedParent}}"
                           StaysOpen="False"
                           Placement="Bottom"
                           HorizontalOffset="{Binding ActualWidth, RelativeSource={RelativeSource TemplatedParent}}"
                           PopupAnimation="Slide"
                           AllowsTransparency="True">
                        <Border Background="#FFF0F0D5"
                                            CornerRadius="0,10,10,10"
                                            Padding="10,6"
                                            BorderThickness="1"
                                            BorderBrush="Black" >
                            <ContentPresenter />
                        </Border>
                    </Popup>
                </Grid>
            </ControlTemplate>
        </ResourceDictionary>
    </UserControl.Resources>

    <Grid Grid.IsSharedSizeScope="True">
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="Auto"/>
        </Grid.RowDefinitions>
        
        <!--TITULO Y ENCABEZADO-->
        <StackPanel Grid.Row="0">
            <TextBlock Text="Informe por Año"
                       Style="{StaticResource TitleTextBlock}"/>

            <!-- EMPRESA, REPORTE Y MULTIPLO -->
            <Grid Height="52">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="*"/>
                    <ColumnDefinition Width="*"/>
                    <ColumnDefinition Width="110"/>
                    <ColumnDefinition Width="Auto"/>
                </Grid.ColumnDefinitions>

                <ComboBox ItemsSource="{Binding Empresas}" 
                          SelectedItem="{Binding SelectedEmpresa}"
                          Grid.Column="0"
                          Style="{StaticResource EmpresaComboBox_App}"/>
                <ComboBox ItemsSource="{Binding SelectedEmpresa.Reportes}" 
                          SelectedItem="{Binding SelectedReporte}"
                          Grid.Column="1"
                          Style="{StaticResource ReporteComboBox_App}"/>
                <ComboBox x:Name="Multiplo_cmb"
                          Grid.Column="2"
                          ItemsSource="{Binding Multiplos}"
                          SelectedItem="{Binding SelectedMultiplo}"
                          be:FocusLastElementBehavior.Enable="False"
                          Style="{StaticResource MultiploComboBox_App}"/>
                
                <ToggleButton Grid.Column="3"
                              IsTabStop="False"
                              Template="{StaticResource settings}">
                    <ScrollViewer MaxWidth="250" MaxHeight="{Binding ActualHeight, ElementName=main_sv, Mode=OneWay}">
                        <StackPanel>
                            <GroupBox Header="Columnas:">
                                <Grid>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition/>
                                        <ColumnDefinition/>
                                    </Grid.ColumnDefinitions>
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="Auto"/>
                                        <RowDefinition Height="Auto"/>
                                        <RowDefinition Height="Auto"/>
                                        <RowDefinition Height="Auto"/>
                                        <RowDefinition Height="Auto"/>
                                        <RowDefinition Height="Auto"/>
                                    </Grid.RowDefinitions>

                                    <CheckBox x:Name="EnableColAno9"
                                              Grid.Row="0" Grid.Column="0"
                                              IsChecked="{Binding Source={x:Static p:Settings.Default}, Path=EnableColAno9, Mode=TwoWay}"
                                              Content="{Binding Reporte8.Fecha, Converter={conv:RestarUnAnoConverter}}"/>
                                    <CheckBox x:Name="EnableColAno8"
                                              Grid.Row="0" Grid.Column="1"
                                              IsChecked="{Binding Source={x:Static p:Settings.Default}, Path=EnableColAno8, Mode=TwoWay}"
                                              Content="{Binding Reporte7.Fecha, Converter={conv:RestarUnAnoConverter}}"/>
                                    <CheckBox x:Name="EnableColAno7"
                                              Grid.Row="1" Grid.Column="0"
                                              IsChecked="{Binding Source={x:Static p:Settings.Default}, Path=EnableColAno7, Mode=TwoWay}"
                                              Content="{Binding Reporte6.Fecha, Converter={conv:RestarUnAnoConverter}}"/>
                                    <CheckBox x:Name="EnableColAno6"
                                              Grid.Row="1" Grid.Column="1"
